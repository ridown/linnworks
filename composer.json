{
    "name": "ridown/linnworks",
    "description": "Linnworks API wrapper",
    "license": "MIT",
    "authors": [
        {
            "name": "Onfuro",
            "email": "parth@onfuro.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Ridown\\Linnworks\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ridown\\Linnworks\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ridown\\Linnworks\\LinnworksServiceProvider"
            ],
            "aliases": {
                "Linnworks": "Ridown\\Linnworks\\LinnworksFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
